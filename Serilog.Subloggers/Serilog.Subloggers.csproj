<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFramework>netstandard2.1</TargetFramework>
    <Nullable>enable</Nullable>
  </PropertyGroup>
  <PropertyGroup>
     <PackageVersion>1.2.0</PackageVersion>
    <AssemblyVersion>1.2.0.0</AssemblyVersion>
        <FileVersion>1.2.0.0</FileVersion>
  </PropertyGroup>
  <PropertyGroup>
    <Title>Serilog Subloggers using Filters</Title>
    <Authors>David Ruiz</Authors>
    <Product>Serilog</Product>
    <Description>Library with Serilog filters and eventid enrichment</Description>
    <PackageReadmeFile>Readme.md</PackageReadmeFile>
    <PackageTags>ILogging Serilog eventid Subloggers</PackageTags>
    <PackageReadmeFile>README.md</PackageReadmeFile>
    <PackageReleaseNotes>This library provides Serilog Subloggers configuration for separate logs in different purposes like, Security, System, Business, Analytics or Time metrics.</PackageReleaseNotes>
    <PackageProjectUrl>https://github.com/druizgui/SerilogSubloggers</PackageProjectUrl>
    <RepositoryUrl>https://github.com/druizgui/SerilogSubloggers</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
    <PackageLicenseFile>LICENSE</PackageLicenseFile>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="Microsoft.Extensions.Logging.Abstractions" Version="(3.*,)" />
    <PackageReference Include="Serilog" Version="(2.*,)" />
  </ItemGroup>
  <ItemGroup>
    <None Include="README.md" Pack="true" PackagePath="\" />
    <None Include="LICENSE"   Pack="true" PackagePath="\" />
  </ItemGroup>
  
</Project>
